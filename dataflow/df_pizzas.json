{
	"name": "df_pizzas",
	"properties": {
		"folder": {
			"name": "DW"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "pizzas_csv",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dim_pizzas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "sort1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          pizza_id as string,",
				"          pizza_type_id as string,",
				"          size as string,",
				"          price as decimal(10,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sort(asc(pizza_id, true)) ~> sort1",
				"sort1 aggregate(groupBy(pizza_id),",
				"     each(match(name != 'pizza_id'), $$ = first($$))) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          pizza_id as string,",
				"          pizza_type_id as string,",
				"          size as string,",
				"          price as decimal(9,2)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          pizza_id,",
				"          pizza_type_id,",
				"          size,",
				"          price",
				"     )) ~> sink1"
			]
		}
	}
}